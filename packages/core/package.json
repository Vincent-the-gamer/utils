{
  "name": "@vincent-the-gamer/utils",
  "type": "module",
  "version": "0.2.0",
  "description": "Opinionated utils of JS/TS by @Vincent-the-gamer",
  "author": "Vincent-the-gamer <erxianqiaottjt@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/Vincent-the-gamer/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vincent-the-gamer/utils.git"
  },
  "bugs": {
    "url": "https://github.com/Vincent-the-gamer/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "client": [
        "./dist/client/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "scripts": {
    "dev": "esno",
    "build": "tsup"
  },
  "dependencies": {
    "aes-js": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.16.15",
    "esno": "^4.8.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  }
}
